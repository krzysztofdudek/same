{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Watch CLI",
			"type": "npm",
			"script": "watch",
			"problemMatcher": "$tsc-watch",
			"isBackground": true,
			"presentation": {
				"reveal": "never"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"path": "./src/cli"
		},
		{
			"label": "Test CLI",
			"type": "npm",
			"script": "test",
			"path": "src/cli",
			"group": "test",
			"problemMatcher": [],
		}
	]
}
