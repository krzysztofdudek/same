{
  "plantuml.render": "PlantUMLServer",
  "plantuml.server": "http://localhost:65100",
  "markdown.preview.scrollEditorWithPreview": false,
  "markdown.preview.scrollPreviewWithEditor": false,
  "html.validate.scripts": false,
  "todo-tree.regex.regexCaseSensitive": true,
  "todo-tree.general.tags": ["@todo", "@issue", "@question"],
  "todo-tree.tree.showCountsInTree": true,
  "todo-tree.tree.groupedByTag": true,
  "todo-tree.general.statusBar": "tags",
  "extensions.ignoreRecommendations": false,
  "c4.diagram.plantuml.enabled": true,
  "cSpell.words": ["enduml", "plantuml", "puml", "startuml", "startsalt", "endsalt", "startgantt", "endgantt", "startmindmap", "endmindmap", "startwbs", "endwbs", "startjson", "endjson", "startyaml", "endyaml", "breakpage"],
  "yaml.schemas": {
      "meta/manifest.json": "manifest.yaml",
      "meta/feature.json": [
        "feature.yaml",
        "*.feature.yaml"
      ],
      "meta/feature-phase.json": [
        "feature-phase.yaml",
        "*.feature-phase.yaml"
      ],
      "meta/component.json": [
        "component.yaml",
        "*.component.yaml"
      ],
      "meta/component-version.json": [
        "component-version.yaml",
        "*.component-version.yaml"
      ]
  }
}
